{% include '@components/_header_search.twig' %}

<main class="page--page py-5 page--page--about-us-v2">

  <div class="d-none d-md-block">
    {{ page.socials }}
  </div>

  <div class="container">

    {{ page.content }}

  </div>


</main>

<div class="bottom-about-us-v2 py-5">

  <div class="container">

    <div class="row g-4">

      <div class="col-lg-6">

        <div class="d-flex gap-4 flex-column flex-sm-row text-center text-sm-start">

          <h3 class="d-block d-sm-none">{{ 'Our vision' }}</h3>


          <div>
            <img alt="" src="/{{ directory }}/image/icons/outlook.svg" />
          </div>

          <div>
            <h3 class="d-none d-sm-block">{{ 'Our vision' }}</h3>
            <p>{{ 'Our vision is for a healthier future for Australians through connected healthcare and to create a collaborative environment to accelerate adoption and use of innovative digital services and technologies.' }}</p>
          </div>

        </div>

      </div>

      <div class="col-lg-6">

        <div class="d-flex gap-4 flex-column flex-sm-row text-center text-sm-start">
          <h3 class="d-block d-sm-none">{{ 'Our mission' }}</h3>

          <div>
            <img alt="" src="/{{ directory }}/image/icons/dart.svg" />
          </div>

          <div>
            <h3 class="d-none d-sm-block">{{ 'Our mission' }}</h3>
            <p>{{ 'To create a collaborative environment to accelerate adoption and use of innovative digital services and technologies.' }}</p>
          </div>

        </div>

      </div>

    </div>

  </div>

</div>

